			("obscured", "\xf75f")
		] ["-t", "<fn=1>\xf2c9</fn>  <tempC>Â°C | <fn=1><skyConditionS></fn>"] 18000,
		Run Date "<fn=1>\xf133</fn>  %m/%d/%Y" "date" 36000,
		Run BatteryP ["BAT0"] ["-t", "<fn=1>\xe0b7</fn>  <left>%"] 600
	],
	sepChar = "%",
	alignSep = "}{",
